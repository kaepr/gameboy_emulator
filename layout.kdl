layout {
    default_tab_template {
        children
        pane size=1 borderless=true {
            plugin location="zellij:compact-bar"
        }
    }
    tab name="nvim" focus=true {
        pane command="nvim" {
            args "."
        }
    }
    tab name="run" split_direction="vertical" {
        pane
        pane
    }
    tab name="git" {
        pane command="gitui"
    }
}


